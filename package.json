{
  "name": "getset",
  "version": "1.0.1",
  "description": "An application for creating, validating, reusing and extending sets of clinical codes. https://getset.herokuapp.com",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -kill-others-on-fail \"npm run server\" \"npm run client\"",
    "debug": "concurrently -kill-others-on-fail \"npm run server:debug\" \"npm run client\"",
    "client": "webpack-dev-server --mode development --open --port 8887",
    "server": "nodemon src/server/index.js",
    "server:debug": "nodemon --inspect-brk src/server/index.js",
    "build": "webpack --mode development",
    "build-prod-server": "webpack --mode production",
    "heroku-postbuild": "npm run build-prod-server",
    "predebug": "npm run build:dev",
    "test": "mocha --recursive test",
    "test-debug": "mocha --recursive --inspect-brk test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rw251/getset.git"
  },
  "author": "Richard Williams",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rw251/getset/issues"
  },
  "engines": {
    "npm": "5.x.x",
    "node": "8.x.x"
  },
  "homepage": "https://getset.herokuapp.com",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-replace-webpack-plugin": "^2.5.5",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.2.2",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.0.1",
    "rollbar-deploy-webpack-plugin": "^1.1.1",
    "rollbar-sourcemap-webpack-plugin": "^2.5.0",
    "sass-loader": "^7.1.0",
    "standard": "^8.6.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.1.1",
    "btoa": "^1.1.2",
    "clusterize.js": "^0.18.1",
    "compression": "^1.7.3",
    "debug": "^2.6.6",
    "dotenv": "^4.0.0",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-force-ssl": "^0.3.2",
    "express-pino-logger": "^2.0.0",
    "express-session": "^1.15.6",
